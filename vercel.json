{
  "buildCommand": "pnpm run build",
  "outputDirectory": "apps/web/.next",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://your-nextpress-api.vercel.app"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://your-nextpress-api.vercel.app/api/$1"
    }
  ]
}
