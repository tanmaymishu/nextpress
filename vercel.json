{
  "buildCommand": "cd apps/web && pnpm install --no-frozen-lockfile && pnpm run build",
  "installCommand": "pnpm install --no-frozen-lockfile",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://nextpress-git-develop-tanmaymishus-projects.vercel.app"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://nextpress-git-develop-tanmaymishus-projects.vercel.app/api/$1"
    }
  ]
}
