{
  "buildCommand": "cd ../.. && pnpm install && pnpm run build:web",
  "installCommand": "cd ../.. && pnpm install",
  "outputDirectory": ".next",
  "env": {
    "NEXT_PUBLIC_API_URL": "https://nextpress.onrender.com"
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://nextpress.onrender.com/api/$1"
    }
  ]
}
